{
  "name": "@reintroducing/auto-test",
  "version": "0.3.0",
  "description": "Testing auto out.",
  "main": "index.js",
  "scripts": {
    "v": "auto version",
    "release2": "./scripts/release.sh",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "auto changelog && npm run version && npm run publish && auto release",
    "ship": "auto shipit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reintroducing/auto-test.git"
  },
  "author": "MP",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reintroducing/auto-test/issues"
  },
  "homepage": "https://github.com/reintroducing/auto-test#readme",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "9.54.0",
    "@auto-it/first-time-contributor": "9.54.0",
    "@auto-it/jira": "9.54.0",
    "@auto-it/slack": "9.54.0",
    "auto": "9.54.0"
  },
  "auto": {
    "baseBranch": "main",
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor",
      "jira",
      "released",
      "slack"
    ],
    "onlyPublishWithReleaseLabel": true
  }
}
